{# Purpose of this template: Display images within an external context #}
<dl>
    {% for image in items %}
        <dt>{{ image|rklandingpagesmodule_formattedTitle }}</dt>
        <dd><a href="{{ path('rklandingpagesmodule_image_display', {'id': image.getId()}) }}">{{ __('Read more') }}</a>
        </dd>
    {% else %}
        <dt>{{ __('No entries found.') }}</dt>
    {% endfor %}
</dl>
